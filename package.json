{
    "name": "portafolio-personal",
    "version": "2.3.2",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "info": "node -v && pnpm -v && npx prisma -v && pm2 -v",
        "check": "pnpm lint && prisma validate",
        "begin": "pnpm pm2:destroy && rm -rf .next && pnpm build && pnpm pm2:start",
        "reload": "pnpm build && pm2 reload ecosystem.config.js",
        "pm2:start": "pm2 start ecosystem.config.js",
        "pm2:destroy": "pm2 stop ecosystem.config.js && pm2 delete ecosystem.config.js",
        "pm2:restart": "pm2 restart ecosystem.config.js",
        "pm2:logs": "pm2 logs ecosystem.config.js",
        "pm2:list": "pm2 list",
        "pm2:status": "pm2 status ecosystem.config.js",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:studio": "prisma studio",
        "update": "ncu -u && ppnpm install"
    },
    "browserslist": "defaults, not ie <= 11",
    "dependencies": {
        "@headlessui/react": "^2.2.7",
        "@heroicons/react": "^2.2.0",
        "@mdx-js/loader": "^3.1.0",
        "@mdx-js/react": "^3.1.0",
        "@next/mdx": "^15.4.5",
        "@next/third-parties": "^15.4.5",
        "@prisma/client": "^6.13.0",
        "@tailwindcss/postcss": "^4.1.11",
        "@tailwindcss/typography": "^0.5.16",
        "cheerio": "^1.1.2",
        "clsx": "^2.1.1",
        "fast-glob": "^3.3.3",
        "feed": "^5.1.0",
        "framer-motion": "12.23.12",
        "highlight.js": "^11.11.1",
        "lucide-react": "^0.535.0",
        "next": "^15.4.5",
        "next-recaptcha-v3": "^1.5.2",
        "next-themes": "^0.4.6",
        "nodemailer": "^7.0.5",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-icon-cloud": "^4.1.7",
        "rehype-highlight": "^7.0.2",
        "remark-gfm": "^4.0.1",
        "tailwindcss": "^4.1.11",
        "uuid": "^11.1.0",
        "zod": "^4.0.14"
    },
    "devDependencies": {
        "eslint": "^9.32.0",
        "eslint-config-next": "^15.4.5",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "prisma": "^6.13.0",
        "sharp": "0.34.3"
    }
}
